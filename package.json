{
  "name": "@meigetsuid/iomanager",
  "version": "2.1.0",
  "type": "module",
  "main": "dist/cjs/index.js",
  "types": "dist/@types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "engines": {
    "node": ">=21.0.0"
  },
  "scripts": {
    "prettier": "prettier --write .",
    "eslint": "eslint --cache --ext .js,.ts .",
    "eslint:fix": "eslint --cache --ext .js,.ts . --fix",
    "ci": "run-p eslint test",
    "test": "jest",
    "prepack": "run-s build:release",
    "postinstall": "run-s generate",
    "generate": "prisma generate",
    "start:container": "docker compose up -d",
    "wait": "node ./DBWait.js",
    "build": "tsc",
    "postbuild": "node ./add-js-extension.js ./dist",
    "build:release": "run-p build:release:*",
    "build:release:cjs": "tsc -p tsconfig.cjs.json",
    "build:release:esm": "tsc -p tsconfig.esm.json",
    "postbuild:release:esm": "node ./add-js-extension.js ./dist/esm"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MeigetsuID/IOManager.git"
  },
  "author": "Meigetsu",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://idportal.meigetsu.jp/contact"
  },
  "homepage": "https://idportal.meigetsu.jp/",
  "description": "Meigetsu ID API Server IO Manager",
  "publishConfig": {
    "access": "restricted",
    "registry": "https://npm.pkg.github.com/"
  },
  "dependencies": {
    "@meigetsusoft/hash": "^1.0.0-patch01",
    "@prisma/client": "^5.17.0",
    "nodeeasyfileio": "^1.0.3",
    "randomstring": "^1.3.0",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@json-spec/core": "^0.1.12",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.1.0",
    "@types/randomstring": "^1.3.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jest": "^28.7.0",
    "eslint-plugin-node": "^11.1.0",
    "jest": "^29.7.0",
    "mysql2": "^3.11.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.1.0",
    "prisma": "^5.6.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.3.2",
    "util": "^0.12.5"
  }
}
